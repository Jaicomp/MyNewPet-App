create table if not exists "User" (id integer, uid integer, accountEnabled boolean);




Kennel (id integer, name varchar, lati, longi, location)

Rank (id integer, type varchar)

User (id integer, uib integer, accountEnabled boolean)
Publication (id integer, dateCreation date, isAdopted boolean, numComplain integer, lati, longi, isDoneDeal boolean)

Animal(id integer, age integer, sex integer, isCastrated integer, description text, urlImage1 varchar, urlImage2 varchar, urlImage3 varchar, urlImage4 varchar, urlImage5 varchar)

Bleed (id integer, name varchar, needPermission boolean, urlImage varchar)

TypeAnimal (id integer, name varchar, urlImage varchar)


R_Kennel_Rank(idKennel, idRank)
R_Rank_User(idRank, idUser)
R_User_Publication(idUser, idPublication)











